{
  "name": "@activitree/mup-aws-beanstalk",
  "version": "0.9.7",
  "type": "commonjs",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activitree/mup-aws-beanstalk.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && copyfiles -u 1 src/assets/* lib",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/preset-env": "^7.23.9",
    "@types/archiver": "^6.0.2",
    "@types/ejs": "^3.1.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/random-seed": "^0.3.4",
    "@types/shell-escape": "^0.2.2",
    "@types/ssh2": "^1.11.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "chai": "^5.0.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-standard": "^5.0.0",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^10.2.0",
    "shelljs": "^0.8.4",
    "standard": "^17.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.504.0",
    "@aws-sdk/client-auto-scaling": "^3.504.0",
    "@aws-sdk/client-cloudtrail": "^3.504.0",
    "@aws-sdk/client-cloudwatch-events": "^3.504.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.504.0",
    "@aws-sdk/client-ec2": "^3.504.0",
    "@aws-sdk/client-ec2-instance-connect": "^3.504.0",
    "@aws-sdk/client-elastic-beanstalk": "^3.504.0",
    "@aws-sdk/client-iam": "^3.504.0",
    "@aws-sdk/client-s3": "^3.504.0",
    "@aws-sdk/client-ssm": "^3.504.0",
    "@aws-sdk/client-sts": "^3.504.0",
    "@aws-sdk/lib-storage": "^3.504.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "ejs": "^3.1.8",
    "joi": "^17.12.1",
    "lodash": "^4.17.21",
    "random-seed": "^0.3.0",
    "shell-escape": "^0.2.0",
    "ssh2": "^1.15.0",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "ignore": [
      "/node_modules/*"
    ]
  }
}
