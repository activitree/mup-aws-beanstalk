{
  "name": "@activitree/mup-aws-beanstalk",
  "version": "0.6.14",
  "description": "Deploy apps to AWS Elastic Beanstalk using Meteor Up",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activitree/mup-aws-beanstalk.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib --copy-files --source-maps",
    "build:watch": "npm run build -s -- -w",
    "lint": "npm run lint:code && npm run lint:docs",
    "lint:docs": "node ./tests/validate-docs.js",
    "lint:code": "eslint .",
    "pretest": "npm run lint -s",
    "test": "mocha --require babel-register --require babel-polyfill ./tests/*.spec.js"
  },
  "author": "zodern",
  "license": "MIT",
  "keywords": [
    "meteor-up"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.14.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "gfm-code-blocks": "^1.0.0",
    "mocha": "^5.0.0",
    "mup": "^1.5.3",
    "shelljs": "^0.7.8",
    "transform-class-properties": "^1.0.0-beta"
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "aws-sdk": "^2.943.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "ejs": "^2.5.7",
    "joi": "^10.6.0",
    "lodash": "^4.17.21",
    "random-seed": "^0.3.0",
    "shell-escape": "^0.2.0",
    "tar": "^4.0.2",
    "uuid": "^3.1.0"
  }
}
